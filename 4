from github import Github

# Your personal GitHub API access token
access_token = 'YOUR_ACCESS_TOKEN'

# Creating a Github instance
g = Github(access_token)

# Repository information
repo_name = 'my-repo'
repo_description = 'This is my repository created via PyGithub'

# Create the repository
user = g.get_user()
repo = user.create_repo(repo_name, description=repo_description)

print(f"Repository '{repo_name}' successfully created!")
print(f"URL: {repo.html_url}")
